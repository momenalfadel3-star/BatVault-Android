<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup disableIfSdkLessThan="31">
        <exclude domain="sharedprefs" />
        <exclude domain="database" />
        <exclude domain="file" path="." />
    </cloud-backup>
    <device-transfer disableIfSdkLessThan="31">
        <include domain="sharedprefs" />
        <include domain="database" />
        <include domain="file" path="." />
    </device-transfer>
</data-extraction-rules>
